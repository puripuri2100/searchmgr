{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "searchmgr",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "fs": {
        "readFile": true,
        "writeFile": true,
        "readDir": true,
        "exists": true,
        "copyFile": true,
        "createDir": true
      },
      "dialog": {
        "open": true,
        "save": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "http": {
        "all": true
      },
      "path": {
        "all": true
      },
      "process": {
        "all": true
      },
      "os": {
        "all": true
      },
      "clipboard": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "searchmgr",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "site.puripuri2100.searchmgr",
      "icon": [
        "icons/icon.ico",
        "icons/icon.png",
        "icons/icon.icns",
        "icons/Square30x30icon.png",
        "icons/Square44x44icon.png",
        "icons/Square71x71icon.png",
        "icons/Square89x89icon.png",
        "icons/Square107x107icon.png",
        "icons/Square142x142icon.png",
        "icons/Square150x150icon.png",
        "icons/Square284x284icon.png",
        "icons/Square310x310icon.png"
      ]
    }
  }
}
